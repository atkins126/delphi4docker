# bash

#If you need sudo, run this like: export USER_NAME=$USER && sudo bash -E PersonalDockerBuild
#Compress the .wine folder
if ! [ -f ./hostwine.tar.gz ]; then
tar -C /home/${USER_NAME:-$USER} -cvf hostwine.tar.gz .wine
fi

docker build -t personal \
             -f PersonalDockerfile\
             .     
